programa {
  funcao inicio() {
      inteiro contador = 0 , escolhaDoUsuario
      escreva("Digite o valor máximo que vou contar até lá: ")
      leia(escolhaDoUsuario)
      enquanto(contador != escolhaDoUsuario){
        escreva("Estou no número ",contador, ".\n")
        contador++
      }
      }
    }
